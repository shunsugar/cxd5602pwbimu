<?xml version="1.0"?>

<package format="3">
  <name>cxd5602pwbimu</name>
  <version>0.0.0</version>
  <description>ROS2 package for cxd5602pwbimu, a Multi-IMU for SONY SPRESENSE.</description>
  <maintainer email="shin.ioaoi@gmail.com">shunsugar</maintainer>
  <license>Apache-2.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  
  <depend>rclcpp</depend>
  <depend>sensor_msgs</depend>
  <depend>nav_msgs</depend>
  <depend>geometry_msgs</depend>
  <depend>tf2</depend>
  <depend>tf2_ros</depend>
  <depend>tf2_geometry_msgs</depend>
  <depend>serial</depend>
  <depend>eigen3_cmake_module</depend>
  
  <exec_depend>imu_filter_madgwick</exec_depend>
  <exec_depend>eigen3</exec_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
